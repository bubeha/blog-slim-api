{
  "name": "bubeha/blog-api",
  "type": "project",
  "license": "MIT",
  "description": "Blog Api",
  "require": {
    "php": "^8.0",
    "slim/slim": "4.*",
    "slim/psr7": "^1.3",
    "symfony/console": "^5.2",
    "doctrine/orm": "^2.8",
    "ext-json": "*",
    "symfony/dotenv": "^5.2",
    "php-di/php-di": "^6.3"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "squizlabs/php_codesniffer": "^3.5",
    "vimeo/psalm": "^4.3",
    "phpunit/phpunit": "^9.5",
    "slevomat/coding-standard": "^6.4",
    "symfony/var-dumper": "^5.2"
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}

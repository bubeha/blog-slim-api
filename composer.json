{
  "name": "bubeha/blog-api",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "8.0",
    "slim/slim": "4.*",
    "slim/psr7": "^1.3",
    "symfony/console": "^5.2",
    "php-di/slim-bridge": "^3.1",
    "doctrine/orm": "^2.8",
    "laminas/laminas-config-aggregator": "^1.4",
    "ext-json": "*"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "friendsofphp/php-cs-fixer": "^2.17",
    "squizlabs/php_codesniffer": "^3.5",
    "vimeo/psalm": "^4.3",
    "phpunit/phpunit": "^9.5",
    "slevomat/coding-standard": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}

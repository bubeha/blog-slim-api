{
  "name": "bubeha/blog-api",
  "type": "project",
  "license": "MIT",
  "require": {
    "php": "^7.4|^8.0",
    "slim/slim": "4.*",
    "slim/psr7": "^1.3",
    "symfony/console": "^5.2"
  },
  "require-dev": {
    "roave/security-advisories": "dev-master",
    "friendsofphp/php-cs-fixer": "^2.17",
    "squizlabs/php_codesniffer": "3.*",
    "vimeo/psalm": "^4.3",
    "phpunit/phpunit": "^9.5"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
